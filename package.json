{
  "name": "check-flow",
  "version": "0.9.2",
  "description": "A better flow cli with ignoring files from node_modules",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "ava",
    "prebuild": "rimraf bin/",
    "build": "babel src -d bin",
    "preflow": "yarn run build",
    "flow": "node bin/index.js --i=\"node_modules/**/*\" --color=always",
    "lint": "eslint src/ tests/ flow-typed/",
    "prepare": "yarn run build"
  },
  "files": [
    "bin/",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "check-flow": "./bin/index.js"
  },
  "dependencies": {
    "minimatch": "3.0.4",
    "ora": "2.1.0",
    "yargs": "11.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.51",
    "@babel/core": "7.0.0-beta.51",
    "@babel/node": "7.0.0-beta.51",
    "@babel/polyfill": "7.0.0-beta.51",
    "@babel/preset-env": "7.0.0-beta.51",
    "@babel/preset-flow": "7.0.0-beta.51",
    "@babel/register": "7.0.0-beta.51",
    "ava": "1.0.0-beta.6",
    "eslint": "5.0.0",
    "eslint-config-henribeck": "5.0.1",
    "flow-0.65.0": "npm:flow-bin@0.65.0",
    "flow-0.66.0": "npm:flow-bin@0.66.0",
    "flow-0.67.0": "npm:flow-bin@0.67.1",
    "flow-0.68.0": "npm:flow-bin@0.68.0",
    "flow-0.69.0": "npm:flow-bin@0.69.0",
    "flow-0.70.0": "npm:flow-bin@0.70.0",
    "flow-0.71.0": "npm:flow-bin@0.71.0",
    "flow-0.72.0": "npm:flow-bin@0.72.0",
    "flow-0.73.0": "npm:flow-bin@0.73.0",
    "flow-0.74.0": "npm:flow-bin@0.74.0",
    "flow-bin": "0.75.0",
    "rimraf": "2.6.2"
  },
  "peerDependencies": {
    "flow-bin": "^0.75.0"
  }
}
